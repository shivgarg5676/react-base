<?xml version='1.0' encoding='utf-8'?>
<widget id="com.prtc.prtcbusinfo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PRTC Bus Info</name>
    <description>
       PRTC bus time table search
    </description>
    <author email="shivgarg5676@gmail.com" href="http://prtcbusinfo.com">
        PRTC Bus info team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-admob-free" spec="0.25.0">
        <variable name="ADMOB_APP_ID" value="ca-app-pub-5205922302022622~1552385515" />
    </plugin>
    <engine name="android" spec="^7.1.4" />
</widget>
